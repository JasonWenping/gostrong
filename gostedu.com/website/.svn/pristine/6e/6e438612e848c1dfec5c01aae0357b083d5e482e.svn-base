# 咨询管理
gsm_content_advisory:
    pattern: /advisory
    defaults: { _controller: "GostSiteManagerBundle:Advisory:list"}

gsm_content_advisory_add:
    pattern: /advisory/add
    defaults: { _controller: "GostSiteManagerBundle:Advisory:add"}

gsm_content_advisory_edit:
    pattern: /advisory/edit/{id}
    defaults: { _controller: "GostSiteManagerBundle:Advisory:edit"}
    requirements: { id: \d+ }

gsm_content_advisory_del:
    pattern: /advisory/del/{id}
    defaults: { _controller: "GostSiteManagerBundle:Advisory:del"}
    requirements: { id: \d+ }