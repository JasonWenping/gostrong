@charset "utf-8";/* CSS Document */
body{
	margin:0px;
}
a{
	text-decoration:none;
	color:#FFF;
}
ul{
	margin:0;
	padding:0;
	text-decoration:none;
	list-style:none;
}
.log_bg_left{
	position:absolute;
	left:0;
	top:60px;
	z-index:-2;
}
#log_Panel{
	margin:250px auto;
	width:996px;
	height:468px;
}
.log_banner{
	margin:0 auto;
	background:url(~/images/cp-loginPanel_bg.png) no-repeat;
	width:691px;
	height:369px;
}
.log_reflection{
	margin:0 auto;
	background:url(~/images/cp-loginPanel_bg_dao.png) no-repeat;
	width:691px;
	height:99px;
}
.log_shadow{
	position:absolute;
	top:582px;
	z-index:-1;
}
.log_table{
	float:right;
	margin:97px 45px 0 0;
	line-height:25px;
	color:#FFF;
	font-size:18px;
}
.log_table td{
	height:40px;
}



#top_banner{
	height:75px;
	background-color:#00C8AC;
	width:100%;
}
.logo{
	padding:17px 0 0 34px;
	float:left;
}
